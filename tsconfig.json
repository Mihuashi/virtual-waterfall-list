{
  "compilerOptions": {
    "target": "es5",                          
    "module": "commonjs",         
    "moduleResolution": "node",            
    "strict": true,                           
    "esModuleInterop": true,
    "sourceMap": true,
    "noImplicitThis": false,
    "resolveJsonModule": true,
    "downlevelIteration": true
  }
}
